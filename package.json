    "@typescript-eslint/parser": "^5.42.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "husky": "^8.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unbuild": "^1.0.2",
    "vite": "^4.0.1",
    "vitest": "^0.25.3"
  }
